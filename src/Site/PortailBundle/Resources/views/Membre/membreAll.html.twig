{% extends "SitePortailBundle:Membre:gestion_membre.html.twig" %}
{% block contenu %}
<center>

 
<font color="#ff0000"><b><h2> {{message}} </h2></b></font>
<table class="gestion">
<tr>
<th> nom </th>
<th>prenom</th>
<th>email</th>
<th>Types</th>
<th>Telephone</th>
<th>nom de societe</th>

</tr>
{% for m in pagerfanta.currentPageResults %}
<tr>
<td>{{ m.nom_mbr }}</td>
<td>{{ m.prenom_mbr}}</td>
<td>{{ m.email}}</td>
<td>{{ m.type}}</td>
<td>{{ m.tel}}</td>
<td>{{ m.nom_societe}}</td>


</tr>
{% else %}
<tr>
<td>Aucun acteur n'a été trouvé.</td>
</tr>
{% endfor %}
{{ pagerfanta(pagerfanta,'default_translated',{'routeName': 'SitePortailBundle_Admin_listMembre'}) }}
</table>
</center>
{%endblock%} 